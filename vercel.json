{
    "rewrites": [
        {
            "source": "/_/:site/:path*",
            "destination": "http://:site/:path*"
        },
        {
            "source": "/_s/:site/:path*",
            "destination": "https://:site/:path*"
        },
        {
            "source": "/t/:path*",
            "destination": "https://ghsrc.wyf9.top/:path*"
        },
        {
            "source": "/_s/:match/:url*/",
            "destination": "https://:match/:url*/"
        },
        {
            "source": "/ghsrc/:match*",
            "destination": "https://ghsrc.wyf9.top/:match*"
        },
        {
            "source": "/wist/:match*",
            "destination": "https://wist.wyf9.top/:match*"
        },
        {
            "source": "/gh/:match*",
            "destination": "https://github.com/:match*"
        },
        {
            "source": "/openai/:match*",
            "destination": "https://api.openai.com/:match*"
        }
    ]
}