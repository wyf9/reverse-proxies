{
    "rewrites": [
        {
            "source": "/px/:site/:path*",
            "destination": "http://:site/:path*"
        },
        {
            "source": "/pxs/:site/:path*",
            "destination": "https://:site/:path*"
        },
        {
            "source": "/gh/:match*",
            "destination": "https://github.com/:match*"
        },
        {
            "source": "/openai/:match*",
            "destination": "https://api.openai.com/:match*"
        },
        {
            "source": "/google/:match*",
            "destination": "https://google.com/:match*"
        }
    ]
}